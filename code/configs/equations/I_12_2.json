{
    "equation": "q1*q2*r/(4*pi*epsilon*r**3)",
    "num_samples": 10,
    "models": [
       
        {
            "type": "linear",
            "model_path": "clean",
            "load": false,
            "training_type": "clean",
            "fit_args": {
                "epochs": 4000,
                "early_stopping": 100
            },
            "noisy_input_feats": [
                0,
                1,
                2,
                3
            ],
            "extended_data":false,
            "sampling_rate": 3

        }
    ],
    "features": {
        "q1": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        },
        "q2": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        },
        "epsilon": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        },
        "r": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        }
    }
}