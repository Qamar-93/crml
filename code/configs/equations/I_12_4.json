        }
    ],
    "features": {
        "q1": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        },
        "epsilon": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        },
        "r": {
            "type": "float",
            "range": [
                1.0,
                5.0
            ]
        }
    }
}