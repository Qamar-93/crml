{
    "equation": "exp(-(theta/sigma)**2/2)/(sqrt(2*pi)*sigma)",
    "num_samples": 100,
    "models": [
        {
            "type": "linear",
            "model_path": "clean",
            "load": false,
            "training_type": "clean",
            "fit_args": {
                "epochs": 600,
                "early_stopping": 40
            },
            "noisy_input_feats": [
                0,
                1
            ],
            "extended_data": false
        }
        
    ],
    "features": {
        "sigma": {
            "type": "float",
            "range": [
                1.0,
                3.0
            ]
        },
        "theta": {
            "type": "float",
            "range": [
                1.0,
                3.0
            ]
        }
    }
}